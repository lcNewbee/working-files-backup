{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    "add-module-exports",
    "transform-es5-property-mutators",
    [
      "module-alias",
      [
        {
          "src": "./shared",
          "expose": "shared"
        }
      ]
    ]
  ]
}
