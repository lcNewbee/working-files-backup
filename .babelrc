{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    ["transform-runtime", {
      "helpers": true,
      "polyfill": false, // defaults to true
      "regenerator": true // defaults to true
    }],
    "transform-es5-property-mutators",
    [
      "module-alias",
      [
        {
          "src": "./shared",
          "expose": "shared"
        }, {
          "src": "./src",
          "expose": "src"
        }
      ]
    ]
  ]
}
