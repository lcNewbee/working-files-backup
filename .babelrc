{
  "presets": [
    "es2015",
    "es2017",
    "stage-3",
    "react"
  ],
  "env": {
    "development": {
      "presets": [
        // "es2015-loose"
      ]
    }
  },
  "plugins": [
    "add-module-exports",
    ["transform-runtime", {
      "helpers": true,
      "polyfill": false, // defaults to true
      "regenerator": true // defaults to true
    }],
    // "transform-es5-property-mutators",
    [
      "module-alias",
      [
        {
          "src": "./shared",
          "expose": "shared"
        }, {
          "src": "./src",
          "expose": "src"
        }
      ]
    ]
  ]
}
